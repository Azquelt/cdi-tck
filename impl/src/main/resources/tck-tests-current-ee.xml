               </methods>
           </class>
           <class name="org.jboss.jsr299.tck.tests.deployment.lifecycle.broken.failsDuringBeanDiscovery.DeploymentFailureTest">
               <methods>
                   <exclude name="testDeploymentFailsBeforeNotifyingObserversAfterBeanDiscovery"/>
