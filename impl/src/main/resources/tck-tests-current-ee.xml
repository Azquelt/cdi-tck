                   <exclude name="testProcessAnnotatedTypeEventFiredForServlet"/>
                   <exclude name="testProcessAnnotatedTypeEventFiredForServletListener"/>
                   <exclude name="testProcessAnnotatedTypeEventFiredForTagHandler"/>       
